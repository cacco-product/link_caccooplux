<?xml version="1.0" encoding="UTF-8"?>
<services xmlns="http://www.demandware.com/xml/impex/services/2014-09-26">
    <service-credential service-credential-id="oplux_event_v2_credentials">
    </service-credential>

    <service-credential service-credential-id="oplux_event_v3_credentials">
    </service-credential>

    <service-credential service-credential-id="oplux_name_credentials">
    </service-credential>

    <service-profile service-profile-id="oplux_event_v2_profile">
        <timeout-millis>60000</timeout-millis>
        <rate-limit-enabled>false</rate-limit-enabled>
        <rate-limit-calls>0</rate-limit-calls>
        <rate-limit-millis>0</rate-limit-millis>
        <cb-enabled>false</cb-enabled>
        <cb-calls>0</cb-calls>
        <cb-millis>0</cb-millis>
    </service-profile>

    <service-profile service-profile-id="oplux_event_v3_profile">
        <timeout-millis>60000</timeout-millis>
        <rate-limit-enabled>false</rate-limit-enabled>
        <rate-limit-calls>0</rate-limit-calls>
        <rate-limit-millis>0</rate-limit-millis>
        <cb-enabled>false</cb-enabled>
        <cb-calls>0</cb-calls>
        <cb-millis>0</cb-millis>
    </service-profile>

    <service-profile service-profile-id="oplux_name_profile">
        <timeout-millis>60000</timeout-millis>
        <rate-limit-enabled>false</rate-limit-enabled>
        <rate-limit-calls>0</rate-limit-calls>
        <rate-limit-millis>0</rate-limit-millis>
        <cb-enabled>false</cb-enabled>
        <cb-calls>0</cb-calls>
        <cb-millis>0</cb-millis>
    </service-profile>

    <service service-id="oplux_event_services_v2">
        <service-type>HTTP</service-type>
        <enabled>true</enabled>
        <log-prefix>oplux</log-prefix>
        <comm-log-enabled>true</comm-log-enabled>
        <force-prd-enabled>false</force-prd-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>oplux_event_v2_profile</profile-id>
        <credential-id>oplux_event_v2_credentials</credential-id>
    </service>

    <service service-id="oplux_event_services_v3">
        <service-type>HTTP</service-type>
        <enabled>true</enabled>
        <log-prefix>oplux</log-prefix>
        <comm-log-enabled>true</comm-log-enabled>
        <force-prd-enabled>false</force-prd-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>oplux_event_v3_profile</profile-id>
        <credential-id>oplux_event_v3_credentials</credential-id>
    </service>

    <service service-id="oplux_name_services">
        <service-type>HTTP</service-type>
        <enabled>true</enabled>
        <log-prefix>oplux</log-prefix>
        <comm-log-enabled>true</comm-log-enabled>
        <force-prd-enabled>false</force-prd-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>oplux_name_profile</profile-id>
        <credential-id>oplux_name_credentials</credential-id>
    </service>

</services>
